<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MessageTextStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
    <style name="MessageTextStyleSetting">
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/width_text</item>
        <item name="android:text">@string/settings</item>
        <item name="android:textSize">@dimen/text_size</item>
    </style>
    <style name="MessageTextStyleBlack">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/padding_start</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height</item>
    </style>

    <style name="ButtonStyle" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:textColor">@color/black_blue</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="iconPadding">@dimen/top_switch</item>
        <item name="iconGravity">textStart</item>
        <item name="cornerRadius">@dimen/margin_size</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
    <style name="ButtonProblem" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:textColor">@color/black_blue</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/text_button</item>
        <item name="android:letterSpacing">-0.05</item>
        <item name="android:layout_width">@dimen/button_width</item>
        <item name="android:layout_height">@dimen/layout_height</item>
        <item name="iconPadding">@dimen/top_switch</item>
        <item name="cornerRadius">@dimen/width_text</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:insetBottom">@dimen/layout_zero</item>
        <item name="android:insetTop">@dimen/layout_zero</item>
    </style>
    <style name="BackGround">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/black_blue</item>
    </style>
    <style name="LinearButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/layout_zero</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="BackGroundSettings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/black_blue</item>
    </style>
    <style name="ImageButton">
        <item name="android:backgroundTint">@color/black_blue</item>
        <item name="android:layout_width">@dimen/layout_width_button</item>
        <item name="android:layout_height">@dimen/layout_width_button</item>
    </style>

    <style name="ImageButtonBlackArrow">
        <item name="srcCompat">@drawable/back</item>
        <item name="android:tint">@color/white</item>
        <item name="android:backgroundTint">@color/black_blue</item>
        <item name="android:layout_width">@dimen/layout_width</item>
        <item name="android:layout_height">@dimen/layout_width</item>
    </style>
    <style name="switchTheme">
        <item name="android:thumbTint">@color/gray</item>
        <item name="android:trackTint">@color/gray</item>
        <item name="android:layout_width">@dimen/width_text</item>
        <item name="android:layout_height">@dimen/height_layout</item>
    </style>
    <style name="EditTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/layout_height</item>
        <item name="android:textColorHint">@color/black_blue</item>
        <item name="android:textColor">@color/black_blue</item>
        <item name="android:drawablePadding">@dimen/drawable_padding</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:focusable">true</item>
        <item name="android:maxLines">1</item>
        <item name="android:background">@drawable/rounded</item>
        <item name="android:drawableStart">@drawable/black</item>
        <item name="android:hint">@string/search</item>
        <item name="android:inputType">text</item>
    </style>
    <style name="imageEyes">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ClearButtonStyle">
        <item name="android:layout_width">@dimen/width_clear</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/cross</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="TrackBackGround">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TrackLinearLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height</item>
        <item name="android:background">@color/black_blue</item>
    </style>

    <style name="TrackImage">
        <item name="android:layout_width">@dimen/track_image</item>
        <item name="android:layout_height">@dimen/track_image</item>
    </style>

    <style name="TrackText">
        <item name="android:layout_width">@dimen/track_width</item>
        <item name="android:layout_height">@dimen/margin_top_button</item>
        <item name="android:text">@string/test</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_track</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="TrackTextName">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/padding_top</item>
        <item name="android:text">@string/test</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_time</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="TrackWhite">
        <item name="android:layout_width">@dimen/layout_width_button</item>
        <item name="android:layout_height">@dimen/layout_width_button</item>
        <item name="srcCompat">@drawable/white</item>
    </style>
    <style name="TrackSeparator">
        <item name="android:layout_width">@dimen/padding_top</item>
        <item name="android:layout_height">@dimen/padding_top</item>
        <item name="srcCompat">@drawable/separator_white</item>
    </style>
    <style name="problemRight">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="srcCompat">@drawable/notewhite</item>
    </style>
    <style name="problemLeft">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="srcCompat">@drawable/smallwhite</item>
    </style>
    <style name="textProblem">
        <item name="android:layout_width">@dimen/problem_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/wifi</item>
        <item name="android:textSize">@dimen/text_problem</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="textNothing">
        <item name="android:layout_width">@dimen/problem_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/nothing</item>
        <item name="android:textSize">@dimen/text_problem</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="textHistory">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:text">@string/you</item>
        <item name="android:textSize">@dimen/text_problem</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>